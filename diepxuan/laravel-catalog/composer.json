{
  "name": "diepxuan/laravel-catalog",
  "description": "Catalog package for DXPanel",
  "license": "MIT",
  "authors": [
    {
      "name": "Tran Ngoc Duc",
      "email": "ductn@diepxuan.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Diepxuan\\Catalog\\Providers\\CatalogServiceProvider"
      ],
      "aliases": {}
    }
  },
  "require": {
    "diepxuan/laravel-core": "*",
    "diepxuan/laravel-ronaldjack": "*",
    "diepxuan/laravel-log": "*",
    "diepxuan/laravel-magento": "*",
    "diepxuan/laravel-simba": "*",
    "diepxuan/php-charset": "*"
  },
  "autoload": {
    "files": [
      "helpers.php"
    ],
    "psr-4": {
      "Diepxuan\\Catalog\\": "src/",
      "Diepxuan\\Catalog\\Database\\Factories\\": "database/factories/",
      "Diepxuan\\Catalog\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Diepxuan\\Catalog\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}